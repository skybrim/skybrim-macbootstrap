Host 10.*
	GSSAPIAuthentication yes
	GSSAPIDelegateCredentials no

Host *
	ForwardAgent yes
	ServerAliveInterval 60
	ServerAliveCountMax 99999
	TCPKeepAlive no
	ControlMaster auto
	ControlPath ~/.ssh/%h-%p-%r
	ControlPersist 4h
	Compression yes

Host vultr
	HostName 104.224.191.118
	User root
	Port 27850
	IdentityFile ~/.ssh/id_rsa_vultr

Host gcp
	HostName 34.80.117.7
	User root
	Port 22
	IdentityFile ~/.ssh/id_rsa_gcp