[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
[help]
    format = html
[log]
    date = local
[core]
    excludesfile = ~/.gitignore_global
    whitespace = cr-at-eol
    quotepath = false
  	editor = vim
    attributesfile = ~/.gitattributes
[i18n]
    commitencoding = utf-8
    logoutputencoding = utf-8
[gui]
    encoding = utf-8
[push]
    default = current
[grep]
    lineNumber = true
[diff "exif"]
    textconv = exiftool
[diff "plist"]
    textconv = plutil -convert xml1 -o -
[user]
	email = throughskybrim@gmail.com
	name = skybrim
	signingkey = D01670A4FD835EBFBF8300B9C420CE76698779F9
[commit]
	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[diff]
	tool = icdiff
[difftool]
  prompt = false
[difftool "icdiff"]
  cmd = /usr/local/bin/icdiff --line-numbers $LOCAL $REMOTE